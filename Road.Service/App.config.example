		<add key="Edition" value="10990" />
		<!--interface-->
		<add key="InterName" value="SevenRoad" />
		<add key="PrivateKey" value="&lt;RSAKeyValue&gt;&lt;Modulus&gt;YOUR_RSA_MODULUS_HERE&lt;/Modulus&gt;&lt;Exponent&gt;YOUR_RSA_EXPONENT_HERE&lt;/Exponent&gt;&lt;P&gt;YOUR_RSA_P_HERE&lt;/P&gt;&lt;Q&gt;YOUR_RSA_Q_HERE&lt;/Q&gt;&lt;DP&gt;YOUR_RSA_DP_HERE&lt;/DP&gt;&lt;DQ&gt;YOUR_RSA_DQ_HERE&lt;/DQ&gt;&lt;InverseQ&gt;YOUR_RSA_INVERSEQ_HERE&lt;/InverseQ&gt;&lt;D&gt;YOUR_RSA_D_HERE&lt;/D&gt;&lt;/RSAKeyValue&gt;" />
