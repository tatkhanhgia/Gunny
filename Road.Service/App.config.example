		<add key="crosszoneString" value="Data Source=KHANHDUY\SQLEXPRESS;Initial Catalog=Project_Game34;Persist Security Info=True;User ID=sa;Password=YOUR_PASSWORD_HERE" />
		<!--game-->
		<add key="MustStrengthenGold" value="1000" />
